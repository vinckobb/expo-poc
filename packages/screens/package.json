{
  "name": "@monorepo/screens",
  "private": true,
  "version": "0.0.0",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./build/index.js"
    },
    "./*": {
      "types": "./src/*/index.ts",
      "default": "./build/*/index.js"
    }
  },
  "scripts": {
    "build": "rimraf build && tsup",
    "dev": "rimraf build && tsup --watch",
    "dev:android": "npm run dev",
    "dev:ios": "npm run dev",
    "clean": "rimraf build && rimraf node_modules"
  },
  "dependencies": {
    "@monorepo/data-access": "*",
    "@monorepo/types": "*",
    "@monorepo/typescript-config": "*",
    "@react-navigation/native": "^7.0.14",
    "effector": "^23.3.0",
    "effector-react": "^23.3.0",
    "react-i18next": "15.5.1",
    "react-native": "0.76.7"
  },
  "devDependencies": {
    "glob": "~11.0.2",
    "rimraf": "~6.0.1",
    "tsup": "~8.0.1",
    "typescript": "5.5.4"
  }
}
