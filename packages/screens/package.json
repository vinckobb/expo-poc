{
  "private": true,
  "name": "@monorepo/screens",
  "version": "0.0.0",
  "main": "./build/index.js",
  "types": "./src/index.ts",
  "react-native": "./src/index.ts",
  "files": [
    "build",
    "src"
  ],
  "scripts": {
    "dev": "tsup --watch --silent",
    "lint": "eslint --ext js,ts,tsx .",
    "test": "jest",
    "build": "tsup --clean"
  },
  "dependencies": {
    "@monorepo/components": "*"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.8",
    "tsup": "^8.3.5"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "@react-navigation/native": "*",
    "@react-navigation/native-stack": "*"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    },
    "react-native-web": {
      "optional": true
    }
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "outDir": "build",
    "sourcemap": true,
    "dts": true,
    "external": [
      "react",
      "react-native"
    ]
  }
}
